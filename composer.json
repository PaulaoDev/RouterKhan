{
    "name": "paulao-dev/khan",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Khan PHP framework",
            "email": "jskhanframework@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "paulaodev/khan-core": "v1.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "MyApp\\": "app/",
            "Middlewares\\": "middlewares/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:8080 ./public/index.php"
    }
}
